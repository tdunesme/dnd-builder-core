{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "resolvePackageJsonExports": false,
    "esModuleInterop": true,
    "baseUrl": "..",
    "paths": {
      "../src/generated/prisma/*": ["src/generated/prisma/*"]
    }
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node"
    },
    "files": true,
    "transpileOnly": true
  }
}
